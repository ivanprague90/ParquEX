<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.controllers.ResultController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <Label text="RESULT" BorderPane.alignment="CENTER">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
         </top>
         <bottom>
            <Button fx:id="btnRetract" mnemonicParsing="false" onAction="#handleButtonRetract" text="RETRACT" BorderPane.alignment="CENTER_RIGHT">
               <font>
                  <Font size="20.0" />
               </font>
            </Button>
         </bottom>
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <center>
            <HBox BorderPane.alignment="CENTER">
               <children>
                  <VBox fx:id="vbxResult" prefWidth="380.0" spacing="20.0" />
                  <VBox prefWidth="380.0" spacing="20.0">
                     <children>
                        <Label fx:id="lblEssence" alignment="CENTER" contentDisplay="CENTER">
                           <font>
                              <Font size="18.0" />
                           </font></Label>
                        <Label fx:id="lblDescription" wrapText="true" />
                        <Label fx:id="lblProperty" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
